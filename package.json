{
  "name": "za-miniapp-hackathon",
  "private": true,
  "version": "1.0.0",
  "description": "za-miniapp-hackathon",
  "repository": "",
  "license": "UNLICENSED",
  "browserslist": [
    "Android >= 5",
    "IOS >= 9.3",
    "Edge >= 15",
    "Safari >= 9.1",
    "Chrome >= 49",
    "Firefox >= 31",
    "Samsung >= 5"
  ],
  "scripts": {
    "dev": "vite",
    "start": "zmp start",
    "deploy": "zmp deploy",
    "prepare": "husky install && chmod ug+x .husky/*",
    "fix-pre-commit": "lint-staged",
    "format": "prettier-eslint --write \"{,!(node_modules)/**/, !(ui/assets)/**/}*.{tsx, ts}\" && git add -A .",
    "release dry run": "standard-version --release-as patch --dry-run",
    "release as version": "standard-version --release-as  1.1.0 --dry-run",
    "release prerelease": "standard-version --prerelease --dry-run",
    "release major": "standard-version --release-as major",
    "release minor": "standard-version --release-as minor",
    "release patch": "standard-version --release-as patch"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@tanstack/eslint-plugin-query": "^4.29.25",
    "@tanstack/react-query": "^4.29.25",
    "@tanstack/react-query-devtools": "^4.29.25",
    "axios": "0.27.2",
    "axios-retry": "^3.3.1",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.8",
    "lodash-es": "^4.17.21",
    "numeral": "^2.0.6",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-accessible-accordion": "^5.0.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.0",
    "react-slick": "^0.29.0",
    "recoil": "^0.7.7",
    "recoil-nexus": "^0.4.0",
    "uuid": "^9.0.0",
    "zmp-sdk": "^2.27.2",
    "zmp-ui": "^1.7.3"
  },
  "devDependencies": {
    "@types/node": "^20.2.3",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.4.12",
    "cross-env": "^7.0.3",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.8.4",
    "prettier-eslint-cli": "^7.1.0",
    "sass": "^1.63.6",
    "standard-version": "^9.5.0",
    "vite": "^2.6.14"
  }
}
